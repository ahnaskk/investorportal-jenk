{
    "name": "investorportal",
    "version": "1.0.0",
    "description": "master",
    "main": "webpack.mix.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "production": "mix --production",
        "prod": "npm run production",
        "dev": "cross-env APP=* npm run development",
        "test:merchant": "cypress open --config integrationFolder=cypress/integration/merchant_side",
        "test:case": "cypress open --config integrationFolder=cypress/integration/Test_cases",
        "test:market": "cypress open --config integrationFolder=cypress/integration/market_place",
        "test:accounts": "cypress open --config integrationFolder=cypress/integration/Accounts",
        "test:init": "npm install --no-save mochapack@2.0.0 mocha@8.0.1 moxios@0.4.0 cypress@6.2.1 null-loader@4.0.1 webpack-node-externals@2.5.2",
        "cypress": "cypress open",
        "test": "cross-env NODE_ENV=testing mochapack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/Front_End/unit/setup.js tests/Front_End/unit/\\*\\*/\\*.spec.js",
        "test:unit": "jest",
        "test:allpages": "cypress open --config integrationFolder=cypress/integration/TestAllPageAccess"
    },
    "repository": {
        "type": "git",
